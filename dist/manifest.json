{
    "manifest_version": 2,
    "name": "The Simpsons Randomiser",
    "description": "Play episodes of The Simpsons on Disney+",
    "version": "0.1.0",
    "browser_action": {
        "default_icon": "icon-16.png",
        "default_popup": "index.html"
    },
    "icons": {
        "16": "icon-16.png",
        "48": "icon-48.png",
        "128": "icon-128.png"
    },
    "permissions": ["storage", "tabs"]
}
